package Giris;
import java.util.Scanner;


public class Recursive_Ayna {

    public static int ayna(int x){
        System.out.print(x + " ");

        if (x>0){
            ayna(x-5);
            System.out.print(x+" ");
        }
        return x;
    }

    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);
        System.out.print("Hesaplanacak sayıyı giriniz : ");
        int a = input.nextInt();

        ayna(a);
    }
}
